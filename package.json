{
  "name": "react-typescript-template",
  "version": "1.0.0",
  "description": "Boilerplate for react-typescript projects",
  "main": "index.tsx",
  "scripts": {
    "dev-server": "webpack serve --config \"./scripts/webpack.dev.config.js\"",
    "dev": "webpack --watch --config \"./scripts/webpack.dev.config.js\"",
    "dev-build": "webpack --config \"./scripts/webpack.dev.config.js\"",
    "dev-build-analyze": "cross-env ANALYZE_BUILD=true webpack --config \"./scripts/webpack.dev.config.js\"",
    "prod-build": "webpack --config \"./scripts/webpack.prod.config.js\"",
    "prod-build-analyze": "cross-env ANALYZE_BUILD=true webpack --config \"./scripts/webpack.prod.config.js\"",
    "eslint-fix": "eslint --config \"./.eslintrc.js\" --color --fix \"./src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "react",
    "typescript",
    "template"
  ],
  "author": "Searates",
  "license": "ISC",
  "engines": {
    "node": ">=12.16.3 <16.9.0",
    "npm": ">=6.14.4 <7.21.1"
  },
  "devDependencies": {
