    "@svgr/webpack": "^5.5.0",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "enzyme": "^3.11.0",
